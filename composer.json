{
    "name": "linio/spreadsheet-parser",
    "description": "Allows you to parse and import spreadsheet data files",
    "keywords": ["linio", "spreadsheet", "xlsx", "csv", "parser"],
    "type": "library",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.4",
        "ext-zip": "*",
        "doctrine/inflector": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0 || ^8.0",
        "mikey179/vfsStream": "1.4.*",
        "friendsofphp/php-cs-fixer": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Linio\\Component\\SpreadsheetParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Linio\\Component\\SpreadsheetParser\\": "tests"
        }
    }
}
